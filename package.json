{
  "name": "sveltekit-appwrite",
  "version": "1.0.0",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "metawrite": "npm v metawrite dist.tarball | xargs curl | tar -xz",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.68",
    "metawrite": "file:./package"
  },
  "type": "module",
  "description": "Everything you need to build a SvelteKit project powered with Appwrite backend, starter powered by [Increasio](https://increas.io/).",
  "repository": "github:koodeau/sveltekit-appwrite",
  "homepage": "https://increas.io/metawrite",
  "keywords": [
    "svelte",
    "sveltekit",
    "appwrite",
    "metawrite"
  ],
  "author": {
    "name": "Nick Revenko",
    "email": "koodeau@null.net",
    "url": "https://koodeau.me/"
  },
  "logo": {
    "file": "./static/logo.svg"
  },
  "license": "MIT"
}
